<div class="member.signature.body">
{{#if typeParameters}}
    <h4>Type parameters</h4>
    {{> typeParameters}}
{{/if}}

{{#if parameters}}
    <h4>Parameters</h4>
        {{#each parameters}}
            <p>{{#compact}}
                {{#each flags}}
                    <span class="tsd-flag ts-flag{{this}}">{{this}}</span>&nbsp;
                {{/each}}
                {{#if flags.isRest}}<span>...</span>{{/if}}
                {{name}}:&nbsp;
                {{#with type}}{{>type}}{{/with}}
                {{#if defaultValue}}
                    <span>
                    &nbsp;=&nbsp;
                    {{defaultValue}}
                    </span>
                {{/if}}
            {{/compact}}</p>

            {{!-- {{#if type.declaration}}
                {{#with type.declaration}}
                    {{> parameter}}
                {{/with}}
            {{/if}} --}}
        {{/each}}
{{/if}}

{{#unless hideSources}}
    {{> member.sources}}
{{/unless}}
</div>